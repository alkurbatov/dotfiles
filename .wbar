# Purprose:
# Basic config for wbar panel

i: /usr/share/pixmaps/wbar/osxbarback.png
c: wbar --bpress --above-desk --vbar --pos left --offset 0 --isize 52 --idist 5 --nanim 4 --jumpf 0.100000 --falfa 100
t: /10

i: /usr/share/pixmaps/wbar/terminal.png
c: urxvt
t: Terminal

i: /usr/share/pixmaps/wbar/gimp.png
c: gimp
t: Gimp

i: /usr/share/pixmaps/wbar/ooffice.png
c: ooffice
t: OOffice

i: /usr/share/pixmaps/wbar/wbar.png
c: wbar-config
t: Config

