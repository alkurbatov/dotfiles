# Purprose:
# Recompile and install new drivers for my wi-fi card.

#!/bin/bash

DRIVER_DIRECTORY="rtl_92ce_92se_92de_8723ae_88ee_linux_mac80211_0012.0207.2018"

cd ${DRIVER_DIRECTORY}
make clean
make

if [ $? == 0 ]; then 
    make uninstall
    make install
else
    echo "Compilation failed!"
fi

